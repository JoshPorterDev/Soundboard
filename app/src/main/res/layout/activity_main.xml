<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/hank1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hank" />

    <ImageButton
        android:id="@+id/hank4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kahn"
        app:srcCompat="@drawable/hank" />

    <ImageButton
        android:id="@+id/hank3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/hank4"
        app:layout_constraintStart_toEndOf="@+id/hank2"
        app:layout_constraintTop_toBottomOf="@+id/bill"
        app:srcCompat="@drawable/hank" />

    <ImageButton
        android:id="@+id/hank2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boomhauer"
        app:srcCompat="@drawable/hank" />

    <ImageButton
        android:id="@+id/kahn"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dale"
        app:srcCompat="@drawable/kahn" />

    <ImageButton
        android:id="@+id/bill"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/kahn"
        app:layout_constraintStart_toEndOf="@+id/boomhauer"
        app:layout_constraintTop_toBottomOf="@+id/bobby"
        app:srcCompat="@drawable/bill" />

    <ImageButton
        android:id="@+id/boomhauer"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hank1"
        app:srcCompat="@drawable/boomhauer" />

    <ImageButton
        android:id="@+id/dale"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dale" />

    <ImageButton
        android:id="@+id/bobby"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/placeholder"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/dale"
        app:layout_constraintStart_toEndOf="@+id/hank1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bobby" />

</androidx.constraintlayout.widget.ConstraintLayout>